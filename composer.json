{
  "name": "tsrgtm/pageman",
  "description": "A Content Management System (CMS) package for Laravel, providing tools to manage pages, posts, and other content.",
  "keywords": [
    "laravel",
    "cms",
    "pageman",
    "content management"
  ],
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Tusar Gautam",
      "email": "gautamtusar2@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0",
    "illuminate/console": "^11.0",
    "illuminate/database": "^11.0",
    "illuminate/filesystem": "^11.0",
    "illuminate/routing": "^11.0",
    "illuminate/view": "^11.0",
    "illuminate/auth": "^11.0",
    "illuminate/http": "^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0",
    "laravel/pint": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tsrgtm\\Pageman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tsrgtm\\Pageman\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tsrgtm\\Pageman\\Providers\\PagemanServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
